{

  "manifest_version": 2,
  "name": "Simple tabs",
  "description": "",
  "version": "1.0",
  "applications": {
    "gecko": {
      "strict_min_version": "66.0"
    }
  },

  "sidebar_action": {
    "default_icon": "icons/star.png",
    "default_title" : "Simple Tabs",
    "default_panel": "sidebar/panel.html"
  },

  "permissions": ["tabs"],

  "commands": {
    "_execute_sidebar_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+Y"
      }
    }
  }

}
